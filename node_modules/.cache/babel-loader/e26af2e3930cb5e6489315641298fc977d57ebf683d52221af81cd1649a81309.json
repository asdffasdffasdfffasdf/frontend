{"ast":null,"code":"import { Bar, Line } from 'vue-chartjs';\nimport { Chart as ChartJS, Title, Tooltip, Legend, CategoryScale, LinearScale, BarElement, PointElement, LineElement } from 'chart.js';\nimport * as chartConfig from '../assets/chartConfig.js';\nimport * as status from '../assets/status';\nChartJS.register(Title, Tooltip, Legend, CategoryScale, LinearScale, BarElement, PointElement, LineElement);\nexport default {\n  name: 'CombinedChart',\n  components: {\n    Bar,\n    Line\n  },\n  data() {\n    return {\n      options: status.options,\n      options2: chartConfig.options,\n      chartData: {\n        labels: ['용존산소량', '탁도', '수온', 'pH'],\n        datasets: [{\n          label: 'Data One',\n          backgroundColor: ['#f87979', '#81D4FA', '#CE93D8', '#FFD54F'],\n          data: status.getStatusData()\n        }]\n      },\n      lineChartData: chartConfig.data,\n      lineChartData2: chartConfig.data2\n    };\n  }\n};","map":{"version":3,"names":["Bar","Line","Chart","ChartJS","Title","Tooltip","Legend","CategoryScale","LinearScale","BarElement","PointElement","LineElement","chartConfig","status","register","name","components","data","options","options2","chartData","labels","datasets","label","backgroundColor","getStatusData","lineChartData","lineChartData2","data2"],"sources":["C:\\Users\\User\\Desktop\\graduationwork\\src\\components\\BarChart2.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"chart\">\n      <Bar :data=\"chartData\" :options=\"options\" />\n    </div>\n  </div>\n  <div class=\"chart-grid\">\n    <div class=\"chart-item\">\n      <Line :data=\"lineChartData2\" :options=\"options2\" />\n      <h3>용존산소량</h3>\n    </div>\n    <div class=\"chart-item\">\n      <Line :data=\"lineChartData\" :options=\"options2\" />\n      <h3>수온</h3>\n    </div>\n    <div class=\"chart-item\">\n      <Line :data=\"lineChartData\" :options=\"options2\" />\n      <h3>PH</h3>\n    </div>\n    <div class=\"chart-item\">\n      <Line :data=\"lineChartData\" :options=\"options2\" />\n      <h3>탁도</h3>\n    </div>\n  </div>\n\n</template>\n\n<script>\nimport { Bar, Line } from 'vue-chartjs'\nimport { Chart as ChartJS, Title, Tooltip, Legend, CategoryScale, LinearScale, BarElement, PointElement, LineElement } from 'chart.js'\nimport * as chartConfig from '../assets/chartConfig.js'\nimport * as status from '../assets/status'\n\nChartJS.register(\n  Title,\n  Tooltip,\n  Legend,\n  CategoryScale,\n  LinearScale,\n  BarElement,\n  PointElement,\n  LineElement\n)\n\nexport default {\n  name: 'CombinedChart',\n  components: {\n    Bar,\n    Line\n  },\n  data() {\n    return {\n      options: status.options,\n      options2: chartConfig.options,\n      chartData: {\n        labels: ['용존산소량', '탁도', '수온', 'pH'],\n        datasets: [\n          {\n            label: 'Data One',\n            backgroundColor: ['#f87979', '#81D4FA', '#CE93D8', '#FFD54F'],\n            data: status.getStatusData()\n          }\n        ]\n      },\n      lineChartData: chartConfig.data,\n      lineChartData2: chartConfig.data2\n    }\n  }\n}\n</script>\n\n<style>\n\n.status {\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n  background-color: #f8f9fa;\n  padding: 10px;\n  height: 80px; /* Adjust the height as needed */\n  width: 90%;\n  max-width: 100vw;\n  margin: 0 auto;\n}\n\n.status-item {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n}\n\n.status-label {\n  font-size: 16px;\n  font-weight: bold;\n}\n\n.status-value {\n  font-size: 24px;\n  font-weight: bold;\n  color: #007bff;\n  margin-top: 5px;\n}\n.chart {\n  height: 800px;\n  width: 80%;\n  max-width: 100vw;\n  margin: 0 auto;\n}\n.chart-container {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  grid-template-rows: 1fr;\n  gap: 10px;\n  height: 100vh;\n}\n\n.chart-bar {\n  height: 100%;\n  width: 100%;\n  max-width: 100vw;\n  margin: 0 auto;\n}\n\n.chart-grid {\n  display: grid;\n  grid-template-columns: repeat(2, 1fr);\n  grid-template-rows: repeat(2, 1fr);\n  gap: 10px;\n}\n\n.chart-item {\n  height: 100%;\n  width: 100%;\n  max-width: 100%;\n}\n</style>\n"],"mappings":"AA4BA,SAASA,GAAG,EAAEC,IAAG,QAAS,aAAY;AACtC,SAASC,KAAI,IAAKC,OAAO,EAAEC,KAAK,EAAEC,OAAO,EAAEC,MAAM,EAAEC,aAAa,EAAEC,WAAW,EAAEC,UAAU,EAAEC,YAAY,EAAEC,WAAU,QAAS,UAAS;AACrI,OAAO,KAAKC,WAAU,MAAO,0BAAyB;AACtD,OAAO,KAAKC,MAAK,MAAO,kBAAiB;AAEzCV,OAAO,CAACW,QAAQ,CACdV,KAAK,EACLC,OAAO,EACPC,MAAM,EACNC,aAAa,EACbC,WAAW,EACXC,UAAU,EACVC,YAAY,EACZC,WACF;AAEA,eAAe;EACbI,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVhB,GAAG;IACHC;EACF,CAAC;EACDgB,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAEL,MAAM,CAACK,OAAO;MACvBC,QAAQ,EAAEP,WAAW,CAACM,OAAO;MAC7BE,SAAS,EAAE;QACTC,MAAM,EAAE,CAAC,OAAO,EAAE,IAAI,EAAE,IAAI,EAAE,IAAI,CAAC;QACnCC,QAAQ,EAAE,CACR;UACEC,KAAK,EAAE,UAAU;UACjBC,eAAe,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,CAAC;UAC7DP,IAAI,EAAEJ,MAAM,CAACY,aAAa,CAAC;QAC7B;MAEJ,CAAC;MACDC,aAAa,EAAEd,WAAW,CAACK,IAAI;MAC/BU,cAAc,EAAEf,WAAW,CAACgB;IAC9B;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}